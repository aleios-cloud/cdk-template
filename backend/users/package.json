{
  "name": "users",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "test": "yarn test-linter && yarn test-type && yarn test-unit && yarn test-circular",
    "watch": "tsc -w",
    "deploy": "cdk deploy",
    "deploy-production": "cdk deploy --stage production",
    "deploy-staging": "cdk deploy --stage staging",
    "deployedCommit": "cdk deployedCommit",
    "lint-fix": "yarn linter-base-config --fix",
    "lint-fix-all": "yarn lint-fix .",
    "linter-base-config": "eslint --ext=js,ts .",
    "remove": "cdk remove",
    "remove-production": "cdk remove --stage production",
    "remove-staging": "cdk remove --stage staging",
    "test-circular": "yarn depcruise --validate .dependency-cruiser.js .",
    "test-linter": "yarn linter-base-config .",
    "test-type": "tsc --noEmit",
    "test-unit": "vitest run --coverage --passWithNoTests"
  },
  "bin": "bin/users.js",
  "dependencies": {
    "@swarmion-starter/core-contracts": "1.0.0",
    "@swarmion-starter/serverless-configuration": "1.0.0",
    "@swarmion-starter/users-contracts": "1.0.0",
    "@swarmion/serverless-contracts": "0.8.3",
    "@swarmion/serverless-helpers": "^0.15.1",
    "aws-cdk-lib": "2.55.1",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1160.0",
    "constructs": "^10.0.0",
    "http-errors": "^2.0.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.19.0",
    "@types/aws-lambda": "^8.10.109",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@vitest/coverage-c8": "^0.23.1",
    "aws-cdk": "2.55.1",
    "dependency-cruiser": "^11.10.0",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "vitest": "^0.23.1"
  }
}
